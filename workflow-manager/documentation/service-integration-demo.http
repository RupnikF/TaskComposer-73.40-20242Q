@baseWorkflowManager=http://ec2-54-242-198-135.compute-1.amazonaws.com:32080

###
GET {{baseWorkflowManager}}/workflows


###
POST {{baseWorkflowManager}}/workflows
Content-Type: application/yaml

name: "UPLOAD_GENERATED_FILE"
args:
  - bucket:
      default: ""
  - s3_key:
      default: ""
  - aws_access_key:
      default: ""
  - aws_secret_key:
      default: ""
  - aws_session_token:
      default: ""
  - aws_region:
      default: "us-east-1"
steps:
  - get_time:
      service: "ubuntu_service"
      task: "bash"
      input:
        cmd: "date"
  - generate_file:
      service: "ubuntu_service"
      task: "bash"
      input:
        cmd: "echo {time} > /data/hello.txt"
        time: "{get_time.stdout}"
  - upload_file:
      service: "s3_service"
      task: "upload"
      input:
        file_path: "/hello.txt"
        bucket_name: "$args.bucket"
        s3_key: "$args.s3_key"
        aws_access_key: "$args.aws_access_key"
        aws_secret_key: "$args.aws_secret_key"
        aws_region: "us-east-1"
        aws_session_token: "$args.aws_session_token"

